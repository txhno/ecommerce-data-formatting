# Rubick CAST Formatting Environment Variables
# Copy this file to .env and customize

# Application settings
RUBICK_CAST_APP__DEBUG_MODE=false
RUBICK_CAST_APP__MAX_FILE_SIZE_MB=50

# Paths
RUBICK_CAST_PATHS__TEMPLATES_DIR=templates
RUBICK_CAST_PATHS__OUTPUT_DIR=output
RUBICK_CAST_PATHS__TEMP_DIR=/tmp/rubick-cast

# Export settings
RUBICK_CAST_EXPORT__DEFAULT_TEMPLATE=
RUBICK_CAST_EXPORT__OUTPUT_PREFIX=Formatted_
RUBICK_CAST_EXPORT__PRESERVE_UNKNOWN_COLUMNS=false

# Import settings
RUBICK_CAST_IMPORT_CONFIG__SIZE_CHART_FILENAME=sku.xlsx
RUBICK_CAST_IMPORT_CONFIG__PRODUCT_DETAILS_FILENAME=style.xlsx
RUBICK_CAST_IMPORT_CONFIG__OUTPUT_FILENAME=Batch_Merged_With_Types_Values.xlsx
